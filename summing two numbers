#include<iostream>
using namespace std;

	
	int sum( int a, int b)
{	
    int c= a+b;
    return c;
}
int main()
{
	int num1; int num2;
	cout<<"enter first number"<<endl;
	cin>>num1;
	cout<<"enter second number"<<endl;
    cin>>num2;
	cout<<"The sum of two numbers is  "<<sum(num1,num2)<<endl;
	return 0;

}
